{
  "name": "chuck-norris",
  "version": "1.0.0",
  "description": "A full stack app with the chuck norris api",
  "main": "index.js",
  "scripts": {
    "server": "cd ./server && npm install && nodemon server.js",
    "client": "cd ./client && npm install && npm start",
    "start": "concurrently --names \"server, client\" \"npm run server --silent\" \"npm run client --silent\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZeeshaanMaudar/chuck-norris.git"
  },
  "author": "Zeeshaan Maudarbocus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZeeshaanMaudar/chuck-norris/issues"
  },
  "homepage": "https://github.com/ZeeshaanMaudar/chuck-norris#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
